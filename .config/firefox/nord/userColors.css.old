/* nord colortheme */
:root {
  --dimmed_fg: color-mix(in srgb, #7f899d 50%, transparent);
  --normal_fg: #7f899d;
  --select_fg: #c3cde1;
  --normal_bg: #2e3440;
  --select_bd: #a1abbf;
  --select_bg: color-mix(in srgb, currentColor 6%, transparent);
  --shadow: 0 0 4px black;
  --roundness: 16px;
  --roundness-small: 12px;
  --roundness-tiny: 8px;
}
